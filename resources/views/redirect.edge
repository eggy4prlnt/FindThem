<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"
    />
    @if(link.is_template == true)
    <meta name="title" content="{{ link.title }}">
    <meta name="description" content="{{ link.description }}">

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://metatags.io/">
    <meta property="og:title" content="{{ link.title }}">
    <meta property="og:description" content="{{ link.description }}">
    <meta property="og:image" content="{{link.image}}">

    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://metatags.io/">
    <meta property="twitter:title" content="{{ link.title }}">
    <meta property="twitter:description" content="{{ link.description }}">
    <meta property="twitter:image" content="{{link.image}}">
    @endif
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Bit.ly - Click Allow to continue</title>
    <style>
      * {
        padding: 0;
        margin: 0;
      }
      html {
        background-color: #0a355b;
        font-size: 62.5%;
        font-family: Arial;
      }
      .arrow {
        position: absolute;
        background-color: white;
        width: 8rem;
        height: 14rem;
        clip-path: polygon(
          50% 0,
          90% 30%,
          60% 30%,
          60% 80%,
          40% 80%,
          40% 30%,
          10% 30%
        );
        transform: translate(25rem, 10rem);
        animation: arrowMovement 1.5s infinite;
      }
      .logo {
        display: block;
        margin: 10rem auto;
      }
      .logo-wrapper > p {
        text-align: center;
        color: white;
        font-size: 2.4rem;
        font-weight: bold;
        text-shadow: 0 0.5rem 4rem rgba(253, 255, 103, 0.5);
      }

      /*ANIMATION*/
      @keyframes arrowMovement {
        0% {
          transform: translate(25rem, 10rem);
        }
        30% {
          transform: translate(25rem, 7rem);
        }
        50% {
          transform: translate(25rem, 10rem);
        }
        65% {
          transform: translate(25rem, 5rem);
        }
        100% {
          transform: translate(25rem, 10rem);
        }
      }

      /*MEDIA QUERY MANAGER*/
      @media only screen and (max-width: 1024px) {
        .arrow {
          display: none;
        }
        .logo {
          margin: 2rem auto;
        }
      }
      @media only screen and (max-width: 600px) {
        .logo-wrapper > p {
          font-size: 2rem;
        }
      }
    </style>
  </head>
  <body>
    <script>
      function replaceAll(string, search, replace) {
        return string.split(search).join(replace);
      }
    </script>

    <div class="arrow"></div>

    <div id="29773761-a0f0-48e4-9d54-9bdd01634f18"></div>

    <div class="logo-wrapper">
      <img
        class="logo"
        src="https://insprl.com/imgdir/page/bitly-logo.png"
        alt="logo"
      />
      <p>Please press 'Allow' to continue</p>
      <p
        style="font-size: 14px; margin-top: 20px; color: silver"
        id="no_display_msg"
      ></p>
    </div>
    <script src="http://code.jquery.com/jquery-3.4.1.min.js"></script>
